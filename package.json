{
    "name": "jobs-portal-sample",
    "version": "0.0.1",
    "description": "Portal and forms for student jobs with CHC",
    "main": "index.js",
    "author": "Robbie Nichols",
    "license": "MIT",
    "scripts": {
        "start": "NODE_ENV=development npm-run-all --parallel server lint:watch",
        "start:server": "NODE_ENV=dev-server npm-run-all --parallel server:watch",
        "start:watch": "NODE_ENV=development npm-run-all --parallel server:watch",
        "start:local": "NODE_ENV=development LOCAL=true npm-run-all --parallel lint:watch server:watch apiServer:watch",
        "start:windows": "set NODE_ENV=development&&npm-run-all --parallel server lint:watch",
        "start:windows:watch": "set NODE_ENV=development&&npm-run-all --parallel server:watch lint:watch",
        "start:windows:local": "set NODE_ENV=development&&set LOCAL=true&&npm-run-all --parallel lint:watch server:watch apiServer:watch",
        "server": "babel-node server/server.js --mode development",
        "server:test": "NODE_ENV=test babel-node server/server.js --mode production",
        "server:watch": "nodemon server/server.js --exec babel-node",
        "apiServer:watch": "nodemon server/apiServer.js --exec babel-node",
        "server:prod": "NODE_ENV=development server/server.js",
        "lint": "./node_modules/.bin/esw --ext .js,.jsx config/webpack src",
        "lint:watch": "npm run lint -- --watch",
        "build:dev": "NODE_ENV=development babel-node tools/build.js",
        "build:prod": "NODE_ENV=production babel-node tools/build.js",
        "build:prod:report": "NODE_ENV=production REPORT=true babel-node tools/build.js"
    },
    "dependencies": {
        "history": "^4.7.2",
        "lodash.partition": "^4.6.0",
        "material-ui": "1.0.0-beta.32",
        "material-ui-icons": "^1.0.0-beta.17",
        "moment": "^2.20.1",
        "prop-types": "^15.6.0",
        "react": "^16.3.0",
        "react-dom": "^16.3.0",
        "react-loadable": "^5.3.1",
        "react-media": "^1.8.0",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "next",
        "redux": "^3.7.2",
        "redux-form": "^7.3.0",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.38",
        "@babel/core": "^7.0.0-beta.38",
        "@babel/node": "^7.0.0-beta.38",
        "@babel/plugin-transform-react-jsx": "^7.0.0-beta.49",
        "@babel/preset-env": "^7.0.0-beta.38",
        "@babel/preset-es2015": "^7.0.0-beta.38",
        "@babel/preset-es2016": "^7.0.0-beta.38",
        "@babel/preset-es2017": "^7.0.0-beta.38",
        "@babel/preset-react": "^7.0.0-beta.38",
        "@babel/preset-stage-2": "^7.0.0-beta.42",
        "@babel/register": "^7.0.0-beta.42",
        "babel-eslint": "^8.2.1",
        "babel-loader": "8.0.0-beta.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "body-parser": "^1.18.2",
        "colors": "^1.2.1",
        "compression": "^1.7.2",
        "cross-fetch": "^2.0.0",
        "eslint": "^4.19.1",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-import": "^2.10.0",
        "eslint-plugin-react": "^7.3.0",
        "eslint-watch": "^3.1.4",
        "express": "^4.16.2",
        "express-async-errors": "^2.1.2",
        "express-session": "^1.15.6",
        "form-data": "^2.3.2",
        "helmet": "^3.12.0",
        "html-webpack-plugin": "^3.1.0",
        "memorystore": "^1.6.0",
        "multer": "^1.3.0",
        "nodemon": "^1.15.1",
        "npm-run-all": "^4.1.1",
        "open": "^0.0.5",
        "passport": "^0.4.0",
        "passport-saml": "^0.33.0",
        "request": "^2.85.0",
        "stream": "^0.0.2",
        "uuid": "^3.2.1",
        "webpack": "^4.4.1",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-dev-middleware": "^3.1.0",
        "webpack-hot-middleware": "^2.19.1"
    }
}
